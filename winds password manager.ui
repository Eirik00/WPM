<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.Frame" id="main_frame">
    <property name="background">#beeffa</property>
    <property name="cursor">arrow</property>
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="propagate">True</property>
    </layout>
    <child>
      <object class="tk.Label" id="title">
        <property name="activebackground">#beeffa</property>
        <property name="background">#beeffa</property>
        <property name="compound">top</property>
        <property name="font">{Calibri} 20 {}</property>
        <property name="foreground">#000000</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Wind's Password Manager</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">n</property>
          <property type="row" id="1" name="minsize">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="pincode_button">
        <property name="command">test</property>
        <property name="compound">bottom</property>
        <property name="font">{Calibri} 10 {bold}</property>
        <property name="foreground">#000000</property>
        <property name="justify">center</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Enter</property>
        <property name="width">7</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">se</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="entry_pincode">
        <property name="background">#ffffff</property>
        <property name="borderwidth">2</property>
        <property name="font">{Times New Roman Baltic} 11 {}</property>
        <property name="foreground">#000000</property>
        <property name="relief">raised</property>
        <property name="show">â€¢</property>
        <property name="validate">none</property>
        <property name="width">8</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="pincode">
        <property name="background">#beeffa</property>
        <property name="compound">top</property>
        <property name="font">{Segoe UI} 12 {}</property>
        <property name="text" translatable="yes">Enter your PIN</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="info">
        <property name="anchor">center</property>
        <property name="background">#beeffa</property>
        <property name="compound">top</property>
        <property name="font">{Calibri} 12 {}</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Login</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="info_creator">
        <property name="text" translatable="yes">Made by Wind 2020</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="sticky">se</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="frame_6">
        <property name="background">#beeffa</property>
        <property name="height">20</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
